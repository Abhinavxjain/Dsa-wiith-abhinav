class Solution {
public int minEatingSpeed(int[] piles , int h){
  int low=0;
  int high=Arrays.stream(piles).max().getAsInt();
   int ans=high;
  while(low<=high){
   int mid = (low+high)/2;
  if(canFanished(piles,h,mid)){
    ans=mid;
   high=mid-1;}
  else{
   low=mid+1;}
  }
  return ans;
  }
  public boolean canFanished(int[] piles,int h ,int mid){
  long hours=0;
  for(int pile:piles){
     hours+=pile/mid;
     if(pile%mid!=0){
        hours++;
      }
     } return hours<=h;
}
}

