class Solutiion {
 public int[] TwoSums(int[] nums,int target){
  HashMap<Integer,Integer> map = new Hashmap<>();
  for(i=0;i<nums.length;i++){
    if(map.containsKey( target-nums[i]){
     int index = map.get( target -nums[i]);
     return new int[]{index,i} }
   map.put{nums[i],i}
   }
   return new int[]{};
  }
}
